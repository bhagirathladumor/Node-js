<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta-width="viewport" content="width=device-width, initial-scale=1.0">
        <title>User Data</title>
        <link rel="stylesheet" href="/css/style.css">
</head>

<body>

    <div class="container wide">
        <h1>User Data</h1>


        <div class="table-wrapper">
            <table class="data-table fancy-table">
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>Name</th>
                        <th>Email</th>
                    </tr>
                </thead>

                <% usersData.map((user, index)=> { %>
                    <tbody>
                        <tr>
                            <td>
                                <%= user.id %>
                            </td>
                            <td>
                                <%= user.name %>
                            </td>
                            <td>
                                <%= user.email %>
                            </td>
                        </tr>
                    </tbody>
                    <% }) %>
            </table>
        </div>

        <a href="/backUser"><button class="btn" style="margin-top: 25px;">Log-Out</button></a>
    </div>

</body>

</html>